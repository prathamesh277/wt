ue=="")
{
flag=0;
}
str=document.myform.title.value;
if(str=="c"||str=="C")
{
document.myform.t1.value="C";
document.myform.t2.value=444;
}
else if(str=="jsp"||str=="JSP")
else{
document.myform.t1.value="JSP"; document.myform.t2.value=555;
}
{
flag=0;
}
